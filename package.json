{
  "name": "ghostr",
  "description": "Renderless vue framework",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "yarn generate:icons && yarn generate:symlinks",
    "serve": "yarn prepare && vue-cli-service serve",
    "build": "yarn prepare && rollup --config rollup.config.js",
    "lint": "vue-cli-service lint",
    "generate:icons": "vsvg -s ./src/assets/raw-svg -t ./src/assets/compiled-svg --es6",
    "generate:symlinks": "link-module-alias",
    "postinstall": "yarn generate:icons && yarn generate:symlinks"
  },
  "main": "dist/dropdownue.esm.js",
  "module": "dist/dropdownue.umd.js",
  "browser": "dist/dropdownue.min.js",
  "unpkg": "dist/dropdownue.min.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^2.6.8",
    "lodash.camelcase": "^4.3.0",
    "lodash.isempty": "^4.4.0",
    "lodash.upperfirst": "^4.3.1",
    "obj-traverse": "^1.0.0",
    "uuid": "^3.3.2",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.0.7",
    "vuebar": "^0.0.20"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-vue": "^5.2.3",
    "fibers": "^4.0.1",
    "link-module-alias": "^1.2.0",
    "lint-staged": "^8.1.5",
    "modularscale-sass": "^3.0.8",
    "node-sass": "^4.12.0",
    "postcss-aspect-ratio": "^1.0.2",
    "postcss-atrule-bem": "^4.0.0",
    "postcss-css-reset": "^1.0.2",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-easing-gradients": "^3.0.1",
    "postcss-flexbox": "^1.0.3",
    "postcss-font-magician": "^2.2.2",
    "postcss-fontpath": "^1.0.0",
    "postcss-g-index": "^0.1.1",
    "postcss-gradient-transparency-fix": "^3.0.0",
    "postcss-hocus": "^1.0.0",
    "postcss-input-style": "^1.0.0",
    "postcss-light-text": "^0.0.1",
    "postcss-magic-animations": "^0.3.0",
    "postcss-media-minmax": "^4.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-placehold": "^1.2.0",
    "postcss-scss": "^2.0.0",
    "postcss-selector-not": "^4.0.0",
    "postcss-shape": "^0.0.1",
    "postcss-short": "^5.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-vue": "^5.0.1",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "style-resources-loader": "^1.2.1",
    "stylelint-config-standard": "^18.3.0",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-svgicon": "^3.2.6",
    "vue-template-compiler": "^2.6.10"
  },
  "_moduleAliases": {
    "~root": ".",
    "~src": "./src",
    "~components": "./src/components",
    "~assets": "./src/assets",
    "~images": "./src/assets/images",
    "~styles": "./src/assets/styles",
    "~library": "./src/library",
    "~helpers": "./src/helpers",
    "~icons": "./src/assets/compiled-svg",
    "~atoms": "./src/components/0-atoms",
    "~molecules": "./src/components/1-molecules",
    "~organisms": "./src/components/2-organisms",
    "~layouts": "./src/components/3-layouts",
    "~pages": "./src/components/4-pages"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
