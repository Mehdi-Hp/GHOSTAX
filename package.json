{
  "name": "dropdownue",
  "version": "0.1.1",
  "main": "dist/dropdownue.umd.js",
  "module": "dist/dropdownue.esm.js",
  "unpkg": "dist/dropdownue.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/dropdownue.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/dropdownue.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/dropdownue.min.js"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-vue": "^5.2.2",
    "fibers": "^4.0.1",
    "node-sass": "^4.12.0",
    "postcss-aspect-ratio": "^1.0.2",
    "postcss-atrule-bem": "^4.0.0",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-easing-gradients": "^3.0.1",
    "postcss-flexbox": "^1.0.3",
    "postcss-font-magician": "^2.2.1",
    "postcss-fontpath": "^1.0.0",
    "postcss-g-index": "^0.1.1",
    "postcss-gradient-transparency-fix": "^3.0.0",
    "postcss-hocus": "^1.0.0",
    "postcss-input-style": "^1.0.0",
    "postcss-light-text": "^0.0.1",
    "postcss-magic-animations": "^0.3.0",
    "postcss-media-minmax": "^4.0.0",
    "postcss-normalize": "^8.0.0",
    "postcss-placehold": "^1.2.0",
    "postcss-selector-not": "^4.0.0",
    "postcss-shape": "^0.0.1",
    "postcss-short": "^5.0.0",
    "rollup": "^1.14.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-vue": "^5.0.0",
    "sass": "^1.21.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "style-resources-loader": "^1.2.1",
    "vue-template-compiler": "^2.6.10"
  }
}
