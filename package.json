{
  "name": "ghostax",
  "description": "Renderless vue framework",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "yarn generate:icons && yarn generate:symlinks",
    "serve": "yarn prepare && vue-cli-service serve",
    "build": "yarn prepare && vue-cli-service build --modern",
    "publish:netlify": "yarn build && netlify deploy --prod",
    "lint": "vue-cli-service lint",
    "test": "jest --coverage --silent",
    "test:watch": "jest --watch --silent",
    "precommit": "npm run lint && npm test",
    "generate:icons": "vsvg -s ./src/assets/raw-svg -t ./src/assets/compiled-svg --es6",
    "generate:symlinks": "link-module-alias",
    "postinstall": "yarn generate:icons && yarn generate:symlinks"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@ghostax/dropdown": "^2.0.3",
    "@ghostax/pagination": "^1.0.1",
    "@mdx-js/vue-loader": "^1.5.0",
    "add": "^2.0.6",
    "core-js": "^3.3.5",
    "destyle.css": "^1.0.10",
    "eslint-plugin-jest": "^23.0.2",
    "lodash.camelcase": "^4.3.0",
    "lodash.capitalize": "^4.2.1",
    "lodash.isempty": "^4.4.0",
    "lodash.upperfirst": "^4.3.1",
    "marked": "^0.7.0",
    "obj-traverse": "^1.0.0",
    "prismjs": "^1.17.1",
    "spiritjs": "^2.4.1",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-container-query-2": "^0.2.2",
    "vue-fragment": "^1.5.1",
    "vue-prism-component": "^1.1.1",
    "vue-router": "^3.1.3",
    "yarn": "^1.19.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.0",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@vusion/md-vue-loader": "^1.2.4",
    "autoprefixer": "^9.7.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "directory-named-webpack-plugin": "^4.0.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-lerna": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "fibers": "^4.0.2",
    "highlightjs": "^9.12.0",
    "jest": "^24.9.0",
    "lerna": "^3.18.3",
    "link-module-alias": "^1.2.0",
    "lint-staged": "^9.4.1",
    "modularscale-sass": "^3.0.10",
    "node-sass": "^4.13.0",
    "postcss-aspect-ratio": "^1.0.2",
    "postcss-atrule-bem": "^4.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-css-reset": "^1.0.2",
    "postcss-custom-properties": "^9.0.2",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-easing-gradients": "^3.0.1",
    "postcss-flexbox": "^1.0.3",
    "postcss-font-magician": "^2.2.2",
    "postcss-fontpath": "^1.0.0",
    "postcss-g-index": "^0.1.1",
    "postcss-gradient-transparency-fix": "^3.0.0",
    "postcss-hocus": "^1.0.0",
    "postcss-import": "^12.0.1",
    "postcss-input-style": "^1.0.0",
    "postcss-light-text": "^0.0.1",
    "postcss-magic-animations": "^0.3.0",
    "postcss-media-minmax": "^4.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-placehold": "^1.2.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "postcss-selector-not": "^4.0.0",
    "postcss-shape": "^0.0.1",
    "postcss-short": "^5.0.0",
    "sass": "^1.23.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "style-resources-loader": "^1.2.1",
    "stylelint-config-standard": "^19.0.0",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-svgicon": "^3.2.6",
    "vue-template-compiler": "^2.6.10"
  },
  "_moduleAliases": {
    "~root": ".",
    "~src": "./src",
    "~components": "./src/components",
    "~docs": "./src/docs",
    "~core": "./src/components/core",
    "~layout": "./src/components/layout",
    "~pages": "./src/components/pages",
    "~units": "./src/components/units",
    "~images": "./src/assets/images",
    "~styles": "./src/assets/styles",
    "~helpers": "./src/helpers",
    "~icons": "./src/assets/compiled-svg",
    "~packages": "./packages"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
